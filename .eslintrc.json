{
    "env": {
        "node": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc" : 0
    },
    "ignorePatterns": [
        "**/assets/*.js",
        "**/mt-resume/**/*.js",
        "**/invoqwer.github.io/**/*.js"
    ]
}